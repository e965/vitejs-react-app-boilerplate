{
	"name": "vitejs-react-app-boilerplate",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"eslint": "eslint .",
		"prettier": "prettier --write **/*.{html,json,js,ts,tsx,css,scss}",
		"lint-staged": "lint-staged",
		"postinstall": "is-ci || husky install"
	},
	"lint-staged": {
		"*.{html,json,js,ts,tsx,css,scss}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@types/react": "17.0.37",
		"@types/react-dom": "17.0.11",
		"@typescript-eslint/eslint-plugin": "5.5.0",
		"@typescript-eslint/parser": "5.5.0",
		"@vitejs/plugin-react-refresh": "1.3.6",
		"eslint": "8.3.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "7.0.4",
		"is-ci": "3.0.1",
		"lint-staged": "12.1.2",
		"prettier": "2.5.0",
		"sass": "1.44.0",
		"typescript": "4.5.2",
		"vite": "2.6.14",
		"vite-react-jsx": "1.1.2"
	}
}
